{
  "expo": {
    "name": "Rekbr App",
    "slug": "rekbr-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "mobile-rekbr-app",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nyamiteams.rekbrapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/logo-app.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.nyamiteams.rekbrapp",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./src/assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "android": {
            "googleServicesFile": "./google-services.json"
          }
        }
      ]
    ],
    "splash": {},
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7720054e-2089-48f0-830e-31b51b327ed2"
      }
    },
    "owner": "nyami"
  }
}
